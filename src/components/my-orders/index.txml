<view class="py-medium radius-8px">
    <text class="text-medium font-bold align_right">My books</text>
      <text class="text-blue ">View More</text>

  <view class="px-medium pt-medium flex justify-around">
    <order-icon
      tiki:for="{{orderIcons}}"
      isLoading="{{isLoading}}"
      tab="{{index}}"
      onTap="onMoveOrderManagement" 
      value="{{numOrders[item.key]}}"
      src="{{item.src}}" 
      text="{{item.value}}"
      author="{{item.author}}"/>
  </view>
</view>
